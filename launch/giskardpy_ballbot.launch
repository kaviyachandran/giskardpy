<launch>
    <param name="robot_description" textfile="$(find simulate_scenes)/model/freejoint_ball.urdf" />

    <node pkg="giskardpy" type="ballbot_standalone.py" name="giskard" output="screen" />

    <node pkg="giskardpy" type="interactive_marker.py" name="giskard_interactive_marker" output="screen">
        <rosparam param="enable_self_collision">False</rosparam>
        <rosparam param="interactive_marker_chains">
            - [ballbot/world, ballbot/ball_link]
        </rosparam>
    </node>
</launch>